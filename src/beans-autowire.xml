<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address" class="com.hy.ly.autowire.Address" p:city="BeiJing"
		p:street="Haidian"></bean>
	<bean id="car" class="com.hy.ly.autowire.Car" p:brand="BMW"
		p:price="1000000"></bean>
	<bean id="student" class="com.hy.ly.autowire.Student" p:name="李蕾"
		autowire="byType"></bean>
</beans>
