<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--FactoryBean来配置Bean的实例
		class指向：FactoryBean的全类名，
		property：配置的是FactoryBean的属性，但实际返回的实例是FactoryBean的getObject()方法返回的实例。
	 -->
	<bean id="car" class="com.hy.ly.factorybean.CarFactoryBean">
		<property name="brand" value="3w"></property>
	</bean>
</beans>
